{
    "manifest_version": 3,
    "name": "Domain Age Checker",
    "version": "1.0",
    "description": "Check the age of any domain",
    "action": {
        "default_popup": "index.html",
        "default_icon": "domain.png"
    },
    "permissions": [
        "activeTab"
    ],
    "host_permissions": [
        "http://localhost:3000/*"
    ]
}